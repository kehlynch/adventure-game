{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-jsx",
    "prettier"
  ],
  "plugins": ["standard", "flowtype", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1,
    "no-console": "warn"
  }
}
